{
    "name": "darius/supermetrics",
    "description": "Fetch and manipulate JSON data from a fictional Supermetrics Social Network REST API",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Darius Matulionis",
            "email": "darius@matulionis.lt"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.3",
        "nesbot/carbon": "^2.13",
        "symfony/cache": "^4.2",
        "tightenco/collect": "^5.7",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "fzaninotto/faker": "^1.8",
        "phpunit/phpunit": "^8.0",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Supermetrics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true
}
